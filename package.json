{
  "name": "dt-grpc-ts",
  "version": "0.0.1",
  "description": "gRPC client library for Draw Things",
  "author": "Kelly Jerrell",
  "license": "MIT",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest run",
    "test:watch": "vitest --watch --ui",
    "gen-code": "node scripts/genCode.mjs"
  },
  "dependencies": {
    "@grpc/grpc-js": "^1.13.2",
    "@petamoriken/float16": "^3.9.2",
    "cli-progress": "^3.12.0",
    "flatbuffers": "^25.2.10",
    "google-protobuf": "^3.21.4",
    "sharp": "^0.34.4"
  },
  "devDependencies": {
    "@types/cli-progress": "^3.11.6",
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^22.13.14",
    "@vitest/ui": "^3.1.1",
    "fs-extra": "^11.3.0",
    "tsup": "^8.4.0",
    "tsx": "^4.19.3",
    "typescript": "^5.9.2",
    "vitest": "^3.1.1"
  },
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./web": {
      "import": "./dist/web.mjs",
      "require": "./dist/web.js"
    },
    "./grpc": {
      "import": "./dist/generated/index.mjs",
      "require": "./dist/generated/index.js"
    }
  }
}
